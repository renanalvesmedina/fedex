<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC 
		"-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" 
		"struts-config_1_2.dtd">

<struts-config>

	<!-- Action Mappings -->
	<action-mappings>

		<action
            path		= "/recepcaoDescarga/cancelarInicioDescarga"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/cancelarInicioDescarga.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/recepcaoDescarga/cancelarInicioDescargaList.jsp"/>
		</action>

		<action
            path		= "/recepcaoDescarga/manterTiposDispositivosUnitizacao"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/manterTiposDispositivosUnitizacao.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/recepcaoDescarga/manterTiposDispositivosUnitizacaoList.jsp"/>
			<forward name="cadOk" path="/WEB-INF/jsp/recepcaoDescarga/manterTiposDispositivosUnitizacaoCad.jsp"/>
		</action>

		<action
            path		= "/recepcaoDescarga/manterMotivosCancelamentoInicioDescarga"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/manterMotivosCancelamentoInicioDescarga.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/recepcaoDescarga/manterMotivosCancelamentoInicioDescargaList.jsp"/>
			<forward name="cadOk" path="/WEB-INF/jsp/recepcaoDescarga/manterMotivosCancelamentoInicioDescargaCad.jsp"/>
		</action>

		<action
            path		= "/recepcaoDescarga/manterDispositivosUnitizacao"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/manterDispositivosUnitizacao.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/recepcaoDescarga/manterDispositivosUnitizacaoList.jsp"/>
			<forward name="cadOk" path="/WEB-INF/jsp/recepcaoDescarga/manterDispositivosUnitizacaoCad.jsp"/>
		</action>

		<action
            path		= "/recepcaoDescarga/emitirRelatoriosVeiculosChegados"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/emitirRelatoriosVeiculosChegados.jsp"/>
			<forward name="pesqOk" path="/WEB-INF/jsp/recepcaoDescarga/emitirRelatoriosVeiculosChegadosPesq.jsp"/>
		</action>

		<action
            path		= "/recepcaoDescarga/consultarCargasViagem"
			type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/consultarCargasViagem.jsp"/>
			<forward name="pesqOk" path="/WEB-INF/jsp/recepcaoDescarga/consultarCargasViagemPesq.jsp"/>
		</action> 

		<action
            path		= "/recepcaoDescarga/consultarCargasViagemDetalhe"
			type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/consultarCargasViagemDetalhe.jsp"/>
		</action> 

		<action
            path		= "/recepcaoDescarga/consultarCargasViagemDocumentosServico"
			type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/consultarCargasViagemDocumentosServico.jsp"/>
		</action> 
		
		<action
            path		= "/recepcaoDescarga/finalizarDescarga"
			type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/finalizarDescarga.jsp"/>
			<forward name="recepcaoDescargaOk" path="/WEB-INF/jsp/recepcaoDescarga/finalizarDescargaRecepcaoDescarga.jsp"/>
			<forward name="dispositivosSemIdentificacaoOk" path="/WEB-INF/jsp/recepcaoDescarga/finalizarDescargaDispositivosSemIdentificacao.jsp"/>
			<forward name="dispositivosComIdentificacaoOk" path="/WEB-INF/jsp/recepcaoDescarga/finalizarDescargaDispositivosComIdentificacao.jsp"/>
			<forward name="volumeManifestoEntregaOk" path="/WEB-INF/jsp/recepcaoDescarga/finalizarDescargaVolumeManifestoEntrega.jsp"/>
			<forward name="volumeManifestoNacionalOk" path="/WEB-INF/jsp/recepcaoDescarga/finalizarDescargaVolumeManifestoNacional.jsp"/>
			<forward name="relatorioDivergenciasOk" path="/WEB-INF/jsp/recepcaoDescarga/finalizarDescargaDivergenciaReport.jsp"/>
		</action> 


		<action
            path		= "/recepcaoDescarga/descarregarVeiculo"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculo.jsp"/>
			<forward name="recepcaoDescargaOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculoRecepcaoDescarga.jsp"/>
			<forward name="trocarOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculoTrocar.jsp"/>
			<forward name="trocarEquipeOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculoTrocarEquipe.jsp"/>
			<forward name="trocarIntegrantesOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculoTrocarIntegrantes.jsp"/>
			<forward name="documentosOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculoDocumentos.jsp"/>
			<forward name="documentosColetasOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculoDocumentosColetas.jsp"/>
			<forward name="abaDocumentosOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculoAbaDocumentos.jsp"/>
			<forward name="abaDocumentoServicoOk" path="/WEB-INF/jsp/recepcaoDescarga/descarregarVeiculoAbaDocumentoServico.jsp"/>
		</action>

		<action
            path		= "/recepcaoDescarga/iniciarDescarga"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/recepcaoDescarga/iniciarDescarga.jsp"/>
			<forward name="recepcaoDescargaOk" path="/WEB-INF/jsp/recepcaoDescarga/iniciarDescargaRecepcaoDescarga.jsp"/>
			<forward name="integrantesOk" path="/WEB-INF/jsp/recepcaoDescarga/iniciarDescargaIntegrantes.jsp"/>
			<forward name="lacresOk" path="/WEB-INF/jsp/recepcaoDescarga/iniciarDescargaLacres.jsp"/>
			<forward name="fotosOk" path="/WEB-INF/jsp/recepcaoDescarga/iniciarDescargaFotos.jsp"/>
			<forward name="conferenciaLacresOk" path="/WEB-INF/jsp/recepcaoDescarga/iniciarDescargaConferenciaLacres.jsp"/>
		</action>


	</action-mappings>

</struts-config>