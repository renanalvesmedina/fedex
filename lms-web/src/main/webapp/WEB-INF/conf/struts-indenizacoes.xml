            path		= "/indenizacoes/liberarPagamentoIndenizacao"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/indenizacoes/liberarPagamentoIndenizacao.jsp"/>
