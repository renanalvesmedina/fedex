			<forward name="pesqOk" path="/WEB-INF/jsp/indenizacoes/emitirRelatorioTempoMedioIndenizacaoPesq.jsp"/>
		</action> 

		<action
            path		= "/indenizacoes/emitirRelatorioIndenizacoes"
