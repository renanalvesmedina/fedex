            path		= "/indenizacoes/cancelarReciboRIM"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/indenizacoes/cancelarReciboRIM.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/indenizacoes/cancelarReciboRIMList.jsp"/>
		</action>

		<action
            path		= "/indenizacoes/manterMotivosCancelamento"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/indenizacoes/manterMotivosCancelamento.jsp"/>
