			<forward name="cadOk" path="/WEB-INF/jsp/indenizacoes/manterMotivosCancelamentoCad.jsp"/>
		</action>

		<action
            path		= "/indenizacoes/incluirReciboIndenizacao"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/indenizacoes/incluirReciboIndenizacao.jsp"/>
			<forward name="rimOk" path="/WEB-INF/jsp/indenizacoes/incluirReciboIndenizacaoRIM.jsp"/>
			<forward name="rncOk" path="/WEB-INF/jsp/indenizacoes/incluirReciboIndenizacaoRNC.jsp"/>
			<forward name="processoOk" path="/WEB-INF/jsp/indenizacoes/incluirReciboIndenizacaoProcesso.jsp"/>
			<forward name="mdaOk" path="/WEB-INF/jsp/indenizacoes/incluirReciboIndenizacaoMDA.jsp"/>
