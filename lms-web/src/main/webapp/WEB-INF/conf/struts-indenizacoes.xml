	<action-mappings>
	
		<action
            path		= "/indenizacoes/emitirReciboRIM"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/indenizacoes/emitirReciboRIM.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/indenizacoes/emitirReciboRIMList.jsp"/>
		</action>

