			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/indenizacoes/emitirRelatorioIndenizacoes.jsp"/>
			<forward name="pesqOk" path="/WEB-INF/jsp/indenizacoes/emitirRelatorioIndenizacoesPesq.jsp"/>
		</action> 
		
