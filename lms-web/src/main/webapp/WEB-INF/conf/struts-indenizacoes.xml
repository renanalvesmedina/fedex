            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/indenizacoes/informarPagamento.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/indenizacoes/informarPagamentoList.jsp"/>
