            path		= "/franqueados/popupLancamentosFranqueados"
			type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/franqueados/popupLancamentosFranqueados.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/franqueados/popupLancamentosFranqueadosList.jsp"/>
		</action> 

