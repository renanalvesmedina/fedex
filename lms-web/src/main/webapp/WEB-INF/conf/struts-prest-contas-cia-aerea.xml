<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC 
		"-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" 
		"struts-config_1_2.dtd">

<struts-config>

	<!-- Action Mappings -->
	<action-mappings>

		<action
            path		= "/prestContasCiaAerea/consultarPrestacaoContas"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/prestContasCiaAerea/consultarPrestacaoContas.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/prestContasCiaAerea/consultarPrestacaoContasList.jsp"/>
			<forward name="intervaloOk" path="/WEB-INF/jsp/prestContasCiaAerea/consultarPrestacaoContasIntervalo.jsp"/>
			<forward name="demICMSOk" path="/WEB-INF/jsp/prestContasCiaAerea/consultarPrestacaoContasDemICMS.jsp"/>
			<forward name="demVendasOk" path="/WEB-INF/jsp/prestContasCiaAerea/consultarPrestacaoContasDemVendas.jsp"/>
			<forward name="demPrestContasOk" path="/WEB-INF/jsp/prestContasCiaAerea/consultarPrestacaoContasDemonstrativo.jsp"/>
			<forward name="canceladasOk" path="/WEB-INF/jsp/prestContasCiaAerea/consultarPrestacaoContasCanceladas.jsp"/>
		</action>


		<action
            path		= "/prestContasCiaAerea/emitirPrestacaoContas"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/prestContasCiaAerea/emitirPrestacaoContas.jsp"/>
			<forward name="pesqOk" path="/WEB-INF/jsp/prestContasCiaAerea/emitirPrestacaoContasPesq.jsp"/>
		</action>

		<action
            path		= "/prestContasCiaAerea/desmarcarPrestacoesContas"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/prestContasCiaAerea/desmarcarPrestacoesContas.jsp"/>
			<forward name="procOk" path="/WEB-INF/jsp/prestContasCiaAerea/desmarcarPrestacoesContasProc.jsp"/>
		</action>

		<action
            path		= "/prestContasCiaAerea/emitirDecendialAWBSEmitidos"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/prestContasCiaAerea/emitirDecendialAWBSEmitidos.jsp"/>
			<forward name="pesqOk" path="/WEB-INF/jsp/prestContasCiaAerea/emitirDecendialAWBSEmitidosPesq.jsp"/>
		</action>

		<action
            path		= "/prestContasCiaAerea/faturamentoCiaAerea"
            type		= "com.mercurio.adsm.framework.web.struts.BaseAction"
			name		= "Lazy">
			<forward name="mainOk" path="/WEB-INF/jsp/prestContasCiaAerea/manterFaturamentoCiaAerea.jsp"/>
			<forward name="listOk" path="/WEB-INF/jsp/prestContasCiaAerea/manterFaturamentoCiaAereaList.jsp"/>
			<forward name="cadOk" path="/WEB-INF/jsp/prestContasCiaAerea/manterFaturamentoCiaAereaCad.jsp"/>
		</action>

	</action-mappings>

</struts-config>
