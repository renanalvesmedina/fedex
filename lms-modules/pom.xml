		</dependency>
		<dependency>
			<groupId>jboss.jbossas.core-libs</groupId>
			<artifactId>jboss-common-jdbc-wrapper</artifactId>
			<version>4.0.4.GA</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>servlet-api</artifactId>
