<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC
    "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans
  default-autowire="byType"
  default-lazy-init="true"
  default-dependency-check="none"
>

  <bean
      id="lms.tracking.filialTrackingService"
      class="com.mercurio.lms.tracking.service.FilialTrackingService"
  ></bean>

  <bean
      id="lms.tracking.searchTrackingService"
      class="com.mercurio.lms.tracking.service.SearchTrackingService"
  ></bean>

  <bean
      id="lms.tracking.stateTrackingService"
      class="com.mercurio.lms.tracking.service.StateTrackingService"
  ></bean>
  
  <bean
      id="lms.tracking.cityTrackingService"
      class="com.mercurio.lms.tracking.service.CityTrackingService"
  ></bean>
  
  <bean
      id="lms.tracking.serviceTrackingService"
      class="com.mercurio.lms.tracking.service.ServiceTrackingService"
  ></bean>
  
    <bean
      id="lms.tracking.radarSolicitacaoUsuarioService"
      class="com.mercurio.lms.tracking.service.RadarSolicitacaoUsuarioService"
  ></bean>
    
     <bean
      id="lms.tracking.linhaTempoService"
      class="com.mercurio.lms.tracking.service.LinhaTempoService"
  ></bean>
  
  
  <!--
     To include additional bean definitions for Spring in the generated
     application context file, add a file to your XDoclet merge directory
     called spring-beans.xml that contains the <bean></bean> markup.
  -->

</beans>
