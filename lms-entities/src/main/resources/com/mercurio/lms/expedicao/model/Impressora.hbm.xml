<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
<!-- 
	Created by the Middlegen Hibernate plugin 2.1 for ADSM

	http://boss.bekk.no/boss/middlegen/
	http://www.hibernate.org/
-->

<class 
	name="com.mercurio.lms.expedicao.model.Impressora" 
	table="IMPRESSORA"
>
	<meta attribute="implement-equals" inherit="false">true</meta>

	<id
		name="idImpressora"
		type="java.lang.Long"
		column="ID_IMPRESSORA"
	>
		<meta attribute="use-in-equals">true</meta>
		<generator class="sequence">
			<param name="sequence">IMPRESSORA_SQ</param>
		</generator>
	</id>

	<property
		name="nrIp"
		type="java.lang.Long"
		column="NR_IP"
		not-null="false"
		length="10"
	/>

	<property
		name="nrPort"
		type="java.lang.Integer"
		column="NR_PORTA"
		not-null="false"
		length="4"
	/>

	<property
		name="dsCheckIn"
		type="java.lang.String"
		column="DS_CHECK_IN"
		not-null="true"
		length="60"
	/>
	<property
		name="dsLocalizacao"
		type="java.lang.String"
		column="DS_LOCALIZACAO"
		length="60"
	/>
	<property
		name="tpImpressora"
		column="TP_IMPRESSORA"
		length="1"
	>
		<type name="domain">
			<param name="domainName">DM_TIPO_IMPRESSORA</param>
		</type>
	</property>
	<property
		name="dsModelo"
		type="java.lang.String"
		column="DS_MODELO"
		length="60"
	/>
	<property
		name="dsFabricante"
		type="java.lang.String"
		column="DS_FABRICANTE"
		length="60"
	/>
	<property
		name="blEtiquetaNova"
		type="sim_nao"
		column="BL_ETIQUETA_NOVA"
		not-null="true"
		length="1"
	/>

	<!-- Associations -->

	<!-- bi-directional one-to-many association to ImpressoraFormulario -->
	<bag
		name="impressoraFormularios"
		lazy="true"
		inverse="true"
		cascade="none"
	>
		<meta attribute="adsm-parametrized-attribute">com.mercurio.lms.configuracoes.model.ImpressoraFormulario</meta>
		<key>
			<column name="ID_IMPRESSORA" />
		</key>
		<one-to-many 
			class="com.mercurio.lms.configuracoes.model.ImpressoraFormulario"
		/>
	</bag>
	<!-- bi-directional many-to-one association to Filial -->
	<many-to-one
		name="filial"
		class="com.mercurio.lms.municipios.model.Filial"
		not-null="true"
	>
		<column name="ID_FILIAL" />
	</many-to-one>
	<!-- bi-directional many-to-one association to BalanÃ§a -->
	<many-to-one
		name="balanca"
		class="com.mercurio.lms.expedicao.model.Balanca"
		not-null="false"
	>
		<column name="ID_BALANCA" />
	</many-to-one>
</class>
</hibernate-mapping>
