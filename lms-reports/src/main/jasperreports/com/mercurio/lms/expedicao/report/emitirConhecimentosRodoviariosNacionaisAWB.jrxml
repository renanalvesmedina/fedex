	<queryString><![CDATA[select
      f.id_filial, f.SG_FILIAL || ' - ' || p.NM_FANTASIA as header,
      f.SG_FILIAL || ' ' || cnh.NR_CONHECIMENTO || '/' || cnh.DV_CONHECIMENTO as num_ctrc,
      '(' || m.DS_SIMBOLO || ') ' || ds.VL_TOTAL_DOC_SERVICO as vlr_ctrc,
      to_char(ds.DH_EMISSAO,'dd/MM/yyyy') as dt_emissao,
      a.NR_AWB , '(' || m.DS_SIMBOLO || ') ' || a.VL_FRETE as vlr_awb,
      pcd.NM_PESSOA as destinatario, pcr.NM_PESSOA as remetente
from
      conhecimento cnh, DOCTO_SERVICO ds, SERVICO s, CTO_AWB ca, AWB a, moeda m, filial f, pessoa p,
      pessoa pcd, pessoa pcr
where 
      cnh.ID_CONHECIMENTO = ds.ID_DOCTO_SERVICO
      and s.ID_SERVICO = ds.ID_SERVICO
      and s.TP_MODAL = 'R'
      and s.TP_ABRANGENCIA = 'N'
      and cnh.ID_CONHECIMENTO = ca.ID_CONHECIMENTO
      and ca.ID_AWB = a.ID_AWB
      and cnh.TP_SITUACAO_CONHECIMENTO = 'E'
      and f.id_filial = p.id_pessoa
      and cnh.id_filial_origem = f.id_filial
      and ds.id_cliente_destinatario = pcd.id_pessoa
      and ds.id_cliente_remetente = pcr.id_pessoa
order by
    f.SG_FILIAL, cnh.NR_CONHECIMENTO, ds.DH_EMISSAO]]></queryString>

