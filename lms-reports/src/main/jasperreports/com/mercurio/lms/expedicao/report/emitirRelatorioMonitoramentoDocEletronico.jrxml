	<queryString><![CDATA[select f.sg_filial "Origem",
       ds.nr_docto_servico "Numero",
       substr(v3.ds_valor_dominio_i,7,length(v3.ds_valor_dominio_i)-7) "Tipo", 
       ds.dh_emissao "Emissao",
       m.nr_chave "Chave_de_Acesso",
       substr(v1.ds_valor_dominio_i,7,length(v1.ds_valor_dominio_i)-7) "Sit_SEFAZ", 
       substr(v2.ds_valor_dominio_i,7,length(v2.ds_valor_dominio_i)-7) "Sit_LMS",    
       p.nr_identificacao "CPF_CNPJ",
       p.nm_pessoa "Cliente_Tomador",
       ds.vl_total_doc_servico "Frete"
from monitoramento_doc_eletronico m,
     docto_servico ds,
     conhecimento c,
     filial f,
     devedor_doc_serv dds,
     pessoa p,
     dominio d1,
     valor_dominio v1,
     dominio d2,
     valor_dominio v2,
     dominio d3,
     valor_dominio v3
where 
  ds.dh_emissao >= to_date('20/07/2013','dd/mm/yyyy') and ds.dh_emissao < to_date('27/07/2013','dd/mm/yyyy')
  and m.id_docto_servico = ds.id_docto_servico
  and c.id_conhecimento = ds.id_docto_servico
  and ds.id_filial_origem = f.id_filial
  and dds.id_docto_servico = ds.id_docto_servico
  and dds.id_cliente = p.id_pessoa  
  and d1.nm_dominio = 'DM_SITUACAO_DOC_ELETRONICO'
  and v1.id_dominio = d1.id_dominio
  and m.tp_situacao_documento = v1.vl_valor_dominio  
  and d2.nm_dominio = 'DM_STATUS_CONHECIMENTO'
  and v2.id_dominio = d2.id_dominio
  and c.tp_situacao_conhecimento = v2.vl_valor_dominio
  and d3.nm_dominio = 'DM_TIPO_DOCUMENTO_SERVICO'
  and v3.id_dominio = d3.id_dominio
  and ds.tp_documento_servico = v3.vl_valor_dominio
  order by ds.id_filial_origem, ds.nr_docto_servico]]></queryString>

