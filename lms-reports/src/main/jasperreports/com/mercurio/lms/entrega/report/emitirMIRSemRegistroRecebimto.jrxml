	<queryString><![CDATA[SELECT 
M.ID_MIR,
M.NR_MIR,
M.TP_MIR,
M.TP_DOCUMENTO_MIR,
M.DH_EMISSAO,
M.DH_ENVIO,
FO.ID_FILIAL AS ID_FILIAL_ORIGEM,
FO.SG_FILIAL AS SG_FILIAL_ORIGEM,
PO.NM_FANTASIA AS NM_FILIAL_ORIGEM,
FD.ID_FILIAL AS ID_FILIAL_DESTINO,
FD.SG_FILIAL AS SG_FILIAL_DESTINO,
PD.NM_FANTASIA AS NM_FILIAL_DESTINO,
SD.DS_SETOR AS DS_SETOR_DESTINO,
UD.NM_USUARIO AS NM_USUARIO_RECEBIMENTO
FROM MIR M
INNER JOIN FILIAL FO ON FO.ID_FILIAL = M.ID_FILIAL_ORIGEM
INNER JOIN PESSOA PO ON PO.ID_PESSOA = FO.ID_FILIAL
INNER JOIN FILIAL FD ON FD.ID_FILIAL = M.ID_FILIAL_DESTINO
INNER JOIN PESSOA PD ON PD.ID_PESSOA = FD.ID_FILIAL
INNER JOIN SETOR SD ON SD.ID_SETOR = M.ID_SETOR_DESTINO
 LEFT JOIN USUARIO UD ON UD.ID_USUARIO = M.ID_USUARIO_RECEBIMENTO
WHERE DH_RECEBIMENTO IS NULL

  AND (
    EXISTS(
      SELECT * FROM DOCUMENTO_MIR DM1,
        REGISTRO_DOCUMENTO_ENTREGA RDE,
        DOCTO_SERVICO DS1,
        CLIENTE CO1,
        CLIENTE CD1
      WHERE DM1.ID_MIR = M.ID_MIR
        AND RDE.ID_REGISTRO_DOCUMENTO_ENTREGA = DM1.ID_REGISTRO_DOCUMENTO_ENTREGA
        AND DS1.ID_DOCTO_SERVICO = RDE.ID_DOCTO_SERVICO
        AND CO1.ID_CLIENTE = DS1.ID_CLIENTE_REMETENTE
        AND CD1.ID_CLIENTE (+) = DS1.ID_CLIENTE_DESTINATARIO 
    )
    OR
    EXISTS(
      SELECT * FROM DOCUMENTO_MIR DM2,
        RECIBO_REEMBOLSO RR,
        DOCTO_SERVICO DS2,
        CLIENTE CO2,
        CLIENTE CD2
      WHERE DM2.ID_MIR = M.ID_MIR
        AND RR.ID_RECIBO_REEMBOLSO = DM2.ID_RECIBO_REEMBOLSO
        AND DS2.ID_DOCTO_SERVICO = RR.ID_RECIBO_REEMBOLSO
        AND CO2.ID_CLIENTE = DS2.ID_CLIENTE_REMETENTE
        AND CD2.ID_CLIENTE (+) = DS2.ID_CLIENTE_DESTINATARIO
    )
  )

ORDER BY
    FO.SG_FILIAL,
    FO.ID_FILIAL,
    M.TP_MIR,
    FD.SG_FILIAL,
    SD.DS_SETOR,
    M.NR_MIR]]></queryString>

