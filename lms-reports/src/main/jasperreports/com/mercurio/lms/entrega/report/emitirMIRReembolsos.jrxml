	<queryString><![CDATA[SELECT m.id_filial_origem AS id_filial_origem,
         fo.sg_filial AS ds_filial_origem,
         m.nr_mir AS nr_mir,
	moeda.ds_simbolo as ds_simbolo,
         m.dh_emissao AS dh_emissao,
         m.id_setor_origem AS id_setor_origem,
         so.ds_setor AS ds_setor_origem,
         m.id_usuario_criacao AS id_usuario_criacao,
         pessoa_usuariolms_origem.nm_pessoa AS nm_usuario_origem,
         m.id_setor_destino AS id_setor_destino,
         sd.ds_setor AS ds_setor_destino,
         m.id_usuario_recebimento AS id_usuario_recebimento,
         pessoa_usuariolms_destino.nm_pessoa AS nm_usuario_recebimento,
         rr.id_docto_serv_reembolsado AS ID_DOCTO_SERV_REEMBOLSADO,
         ds.id_filial_origem AS ID_FILIAL_ORIGEM_DOCTO_SERVICO,
         fo.sg_filial AS SG_FILIAL_ORIGEM_DOCTO_SERVICO,
         ds.nr_docto_servico AS NR_DOCTO_SERVICO,
         rr.vl_reembolso AS vl_reembolso,
         rr.vl_aplicado AS vl_aplicado,
         rr.tp_valor_atribuido_recibo AS tp_valor_atribuido_recibo,
         ds.id_cliente_remetente AS id_cliente_remetente,
         po.nm_pessoa AS NM_PESSOA_REMETENTE,
         ds.id_cliente_destinatario AS id_cliente_destinatario,
         pd.nm_pessoa AS NM_PESSOA_DESTINATARIO,
         cher.nr_banco AS nr_banco,
         cher.nr_agencia AS nr_agencia,
         cher.nr_cheque AS nr_cheque,
         cher.vl_cheque AS VL_CHEQUE_VAL,
         cher.dt_cheque AS dt_cheque
FROM     mir m,
         filial fo,
         filial fd,
         documento_mir dm,
         recibo_reembolso rr,
         docto_servico ds,
         cliente cr,
         cliente cd,
         pessoa po,
         pessoa pd,
         pessoa pessoa_usuariolms_destino,
         pessoa pessoa_usuariolms_origem,
         setor so,
         setor sd,
         usuario_lms ud,
         usuario_lms uo,
	moeda moeda,
         cheque_reembolso cher
WHERE    m.id_mir = dm.id_mir
         AND dm.id_recibo_reembolso = rr.id_recibo_reembolso
         AND rr.id_docto_serv_reembolsado = ds.id_docto_servico
         AND ds.id_cliente_remetente = cr.id_cliente
         AND cr.id_cliente = po.id_pessoa
         AND ds.id_cliente_destinatario = cd.id_cliente
         AND cd.id_cliente = pd.id_pessoa
         AND m.id_setor_destino = sd.id_setor
         AND m.id_setor_origem = so.id_setor
         AND m.id_usuario_criacao = uo.id_usuario
         AND m.id_usuario_recebimento = ud.id_usuario
         AND pessoa_usuariolms_destino.id_pessoa = ud.id_usuario
         AND pessoa_usuariolms_origem.id_pessoa = ud.id_usuario
         AND m.id_filial_origem = fo.id_filial
	AND moeda.id_moeda = ds.id_moeda
         AND m.id_filial_destino = fd.id_filial
         AND cher.id_recibo_reembolso = dm.id_recibo_reembolso
      
ORDER BY ds.id_filial_origem,
         fo.sg_filial,
         ds.nr_docto_servico]]></queryString>

