	<queryString><![CDATA[-- LMS-1303
-- Relatórios de consumo de CO2
SELECT 'POA' AS filial
,      mt.nr_frota AS nr_frota
,      mt.nr_identificador AS nr_identificador
,      'Gasolina' AS combustivel
,      1000   AS quilometragem
,      'Proprio' AS vinculo
,      'Carro' AS tp_meio_transporte
,      'Terrestre' AS modalidade
FROM   meio_transporte mt
WHERE  ROWNUM <= 100]]></queryString>

