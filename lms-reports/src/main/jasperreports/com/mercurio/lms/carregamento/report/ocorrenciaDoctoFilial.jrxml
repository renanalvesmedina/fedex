	<queryString><![CDATA[SELECT ds.tp_documento_servico AS tp_docto_servico
,      fo.sg_filial            AS filial_docto_servico
,      ds.nr_docto_servico     AS nr_docto_servico
,      fl.sg_filial            AS filial_localizacao
,      fo.sg_filial            AS filial_ocorrencia
,      fd.sg_filial            AS filial_destino
,      to_char( ds.dh_emissao, 'DD-MM-YYYY HH24:MI') AS dt_ocorrencia
,      'Usuário ' || MOD( ds.nr_docto_servico, 10 )  AS ds_login
FROM   docto_servico ds        
JOIN   filial        fo ON fo.id_filial = ds.id_filial_origem
JOIN   filial        fl ON fl.id_filial = ds.id_filial_localizacao
JOIN   filial        fd ON fd.id_filial = ds.id_filial_destino
WHERE  TRUNC( CAST( ds.dh_emissao AS DATE ) ) > SYSDATE - 30
AND    ROWNUM <= 100]]></queryString>

