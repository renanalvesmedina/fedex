         EXTRACTVALUE(VVDI.DS_VALOR_DOMINIO, '/I18N/TEXTO[@locale="pt_BR"]') DS_TP_INDICADOR,
         GC.VL_GENERALIDADE VL_GENERALIDADE,
         M.SG_MOEDA SG_MOEDA,
         M.DS_SIMBOLO DS_SIMBOLO
    FROM PARCELA_PRECO PP,
         GENERALIDADE_CLIENTE GC,
         V_DOMINIO_I VDI, -- DOMINIO DO TP_INDICADOR
         V_VALOR_DOMINIO_I VVDI,
         MOEDA M,
         PARAMETRO_CLIENTE PC,
         TABELA_DIVISAO_CLIENTE TDC,
         TABELA_PRECO TP
   WHERE GC.ID_PARCELA_PRECO = PP.ID_PARCELA_PRECO AND
         VDI.ID_DOMINIO = VVDI.ID_DOMINIO AND -- DOMINIO DO TP_INDICADOR
         VVDI.VL_VALOR_DOMINIO = GC.TP_INDICADOR AND
         VDI.NM_DOMINIO = 'DM_INDICADOR_PARAMETRO_CLIENTE' AND
         PC.ID_TABELA_DIVISAO_CLIENTE = TDC.ID_TABELA_DIVISAO_CLIENTE AND
         TDC.ID_TABELA_PRECO = TP.ID_TABELA_PRECO AND
         TP.ID_MOEDA = M.ID_MOEDA AND
         PC.ID_PARAMETRO_CLIENTE = GC.ID_PARAMETRO_CLIENTE AND
         GC.ID_PARAMETRO_CLIENTE = 67
ORDER BY PP.NM_PARCELA_PRECO]]></queryString>

