gc.vl_generalidade as vl_generalidade
from 
parametro_cliente pc,
cotacao cot,
generalidade_cliente gc,
parcela_preco pp,
(select vdom.vl_valor_dominio as vl, vdom.ds_valor_dominio as ds 
from dominio dom, valor_dominio vdom 
where dom.id_dominio = vdom.id_dominio
and dom.nm_dominio='DM_INDICADOR_PARAMETRO_CLIENTE') tp_ind_dom
