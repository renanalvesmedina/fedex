cot.id_cotacao=1
and cot.id_cotacao=pc.id_cotacao(+)
and pc.id_parametro_cliente=gc.id_parametro_cliente(+)
and pp.id_parcela_preco=gc.id_parcela_preco
and pp.cd_parcela_preco<>'IDPedagio'
