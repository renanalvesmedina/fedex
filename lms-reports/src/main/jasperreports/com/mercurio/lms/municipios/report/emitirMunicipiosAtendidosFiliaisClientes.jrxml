	<queryString><![CDATA[SELECT OSL.ID_OPERACAO_SERVICO_LOCALIZA AS ID_JOIN,PC.NM_PESSOA AS NM_CLIENTE_ESPECIFICO
FROM
OPERACAO_SERVICO_LOCALIZA OSL,
ATENDIMENTO_CLIENTE AC,
CLIENTE C,
PESSOA PC
WHERE
    OSL.ID_MUNICIPIO_FILIAL = 22
AND AC.ID_OPERACAO_SERVICO_LOCALIZA = OSL.ID_OPERACAO_SERVICO_LOCALIZA 
AND AC.DT_VIGENCIA_INICIAL <= SYSDATE AND (AC.DT_VIGENCIA_FINAL >= SYSDATE OR AC.DT_VIGENCIA_FINAL IS NULL)
AND AC.ID_CLIENTE = C.ID_CLIENTE
AND C.ID_CLIENTE = PC.ID_PESSOA
ORDER BY PC.NM_PESSOA]]></queryString>
