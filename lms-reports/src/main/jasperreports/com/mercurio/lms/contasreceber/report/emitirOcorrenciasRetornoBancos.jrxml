	<queryString><![CDATA[SELECT FC.SG_FILIAL AS SG_FILIAL_COBRADORA,
         FC.ID_FILIAL AS ID_FILIAL_COBRADORA,
         PFC.NM_FANTASIA AS NM_FANTASIA_FILIAL_COBRADORA,
         FC.SG_FILIAL || ' - ' ||  PFC.NM_FANTASIA AS NM_FILIAL_COBRADORA,
         C.ID_CEDENTE AS ID_CEDENTE,
         C.DS_CEDENTE AS DS_CEDENTE,
         OB.ID_OCORRENCIA_BANCO AS ID_OCORRENCIA_BANCO,
         OB.NR_OCORRENCIA_BANCO AS NR_OCORRENCIA_BANCO,
         OB.DS_OCORRENCIA_BANCO AS DS_OCORRENCIA_BANCO,
         F.ID_FATURA AS ID_FATURA,
         FO.SG_FILIAL AS SG_FILIAL_ORIGEM,
         F.NR_FATURA AS NR_FATURA,
         B.NR_BOLETO AS NR_BOLETO,
         B.TP_SITUACAO_BOLETO AS TP_SITUACAO_BOLETO,
         P.TP_IDENTIFICACAO AS TP_IDENTIFICACAO,
         P.NR_IDENTIFICACAO AS NR_IDENTIFICACAO,
         P.NM_PESSOA AS NM_PESSOA,
     TO_CHAR(HB.DH_OCORRENCIA, 'dd/MM/yyyy') AS DH_OCORRENCIA,
         HMO.ID_HISTORICO_MOTIVO_OCORRENCIA AS ID_HISTORICO_MOTIVO_OCORRENCIA,
         MOB.NR_MOTIVO_OCORRENCIA_BANCO AS NR_MOTIVO_OCORRENCIA_BANCO,
         MOB.DS_MOTIVO_OCORRENCIA_BANCO AS DS_MOTIVO_OCORRENCIA_BANCO
    FROM FATURA F,
         BOLETO B,
         HISTORICO_BOLETO HB,
         OCORRENCIA_BANCO OB,
         HISTORICO_MOTIVO_OCORRENCIA HMO,
         MOTIVO_OCORRENCIA_BANCO MOB,
         PESSOA P,
         FILIAL FC,
         CEDENTE C,
         FILIAL FO,
         PESSOA PFC,
         BANCO BAN
   WHERE F.ID_BOLETO = B.ID_BOLETO
     AND B.ID_BOLETO = HB.ID_BOLETO
     AND HB.ID_OCORRENCIA_BANCO = OB.ID_OCORRENCIA_BANCO
     AND HB.ID_HISTORICO_BOLETO = HMO.ID_HISTORICO_BOLETO
     AND HMO.ID_MOTIVO_OCORRENCIA_BANCO = MOB.ID_MOTIVO_OCORRENCIA_BANCO
     AND F.ID_CLIENTE = P.ID_PESSOA
     AND F.ID_FILIAL_COBRADORA = FC.ID_FILIAL
     AND F.ID_CEDENTE = C.ID_CEDENTE
     AND F.ID_FILIAL = FO.ID_FILIAL
     AND PFC.ID_PESSOA = FC.ID_FILIAL
     AND OB.ID_BANCO = BAN.ID_BANCO
ORDER BY FC.SG_FILIAL, 
         BAN.NR_BANCO, 
         OB.NR_OCORRENCIA_BANCO,
         FO.SG_FILIAL,
         F.NR_FATURA,
         MOB.NR_MOTIVO_OCORRENCIA_BANCO]]></queryString>

