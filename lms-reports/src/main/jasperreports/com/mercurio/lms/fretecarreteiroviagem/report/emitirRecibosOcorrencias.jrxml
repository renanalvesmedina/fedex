	<queryString><![CDATA[SELECT f.id_filial,
         f.sg_filial,
         pes_fil.nm_fantasia,
         p.id_proprietario,
         pes_prop.tp_pessoa,
         pes_prop.tp_identificacao,
         pes_prop.nr_identificacao,
         pes_prop.nm_pessoa,
         fil_cc_org.sg_filial AS sg_filial_origem,
         cc.nr_controle_carga,
         fil_cc_des.sg_filial AS sg_filial_destino,
         rfc.id_recibo_frete_carreteiro,
         rfc.nr_recibo_frete_carreteiro,
         rfc.dh_emissao,
         rfc.tp_situacao_recibo,
         rfc.dt_pagto_real,
         nvl(rfc.vl_bruto,0) AS vl_bruto,
         nvl(ofc.vl_desconto,0) AS vl_desconto,
         ofc.tp_ocorrencia,
         ofc.dt_ocorrencia_frete_carreteiro,
         ofc.ob_motivo,
         mp.id_pais,
         mp.id_moeda
FROM     ocorrencia_frete_carreteiro ofc,
         recibo_frete_carreteiro rfc,
         proprietario p,
         pessoa pes_prop,
         controle_carga cc,
         filial fil_cc_org,
         filial fil_cc_des,
         filial f,
         pessoa pes_fil,
         moeda_pais mp
WHERE    ofc.id_recibo_frete_carreteiro = rfc.id_recibo_frete_carreteiro
         AND rfc.id_proprietario = p.id_proprietario
         AND p.id_proprietario = pes_prop.id_pessoa
         AND rfc.id_controle_carga = cc.id_controle_carga (+)
         AND cc.id_filial_origem = fil_cc_org.id_filial (+)
         AND cc.id_filial_destino = fil_cc_des.id_filial (+)
         AND rfc.id_filial = f.id_filial
         AND f.id_filial = pes_fil.id_pessoa
         AND mp.id_moeda_pais = rfc.id_moeda_pais
         AND ofc.bl_desconto_cancelado = 'N']]></queryString>

