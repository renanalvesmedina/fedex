<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC
    "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans
  default-autowire="byType"
  default-lazy-init="true"
  default-dependency-check="none"
>

  <bean
      id="lms.prestcontasciaaerea.emitirPrestacaoContasService"
      class="com.mercurio.lms.prestcontasciaaerea.report.EmitirPrestacaoContasService"
  >

    <property name="reportName">
      <value>com/mercurio/lms/prestcontasciaaerea/report/emitirPrestacaoContasDecendial.jasper</value>
    </property>
  </bean>

  <bean
      id="lms.prestcontasciaaerea.emitirDecendialAWBsEmitidosService"
      class="com.mercurio.lms.prestcontasciaaerea.report.EmitirDecendialAWBsEmitidosService"
  >

    <property name="reportName">
      <value>com/mercurio/lms/prestcontasciaaerea/report/emitirDecendialAWBSEmitidos.jasper</value>
    </property>
  </bean>
  
  <bean
      id="lms.prestcontasciaaerea.emitirRelatorioAwbService"
      class="com.mercurio.lms.prestcontasciaaerea.report.EmitirRelatorioAwbService"
  >

    <property name="reportName">
      <value>com/mercurio/lms/prestcontasciaaerea/report/relatorioAwb.jasper</value>
    </property>
  </bean>

  <!--
     To include additional bean definitions for Spring in the generated
     application context file, add a file to your XDoclet merge directory
     called spring-beans.xml that contains the <bean></bean> markup.
  -->

</beans>
