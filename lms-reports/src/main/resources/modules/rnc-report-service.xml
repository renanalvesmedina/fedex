<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC
    "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans
  default-autowire="byType"
  default-lazy-init="true"
  default-dependency-check="none"
>

  <bean
      id="lms.rnc.emitirRNCService"
      class="com.mercurio.lms.rnc.report.EmitirRNCService"
  >

    <property name="reportName">
      <value>com/mercurio/lms/rnc/report/emitirRNC.jasper</value>
    </property>
  </bean>

  <bean
      id="lms.rnc.relatorioEstatisticoNaoConformidadeService"
      class="com.mercurio.lms.rnc.report.RelatorioEstatisticoNaoConformidadeService"
  >

    <property name="reportName">
      <value>com/mercurio/lms/rnc/report/emitirRelatorioEstatisticoNaoConformidade.jasper</value>
    </property>
  </bean>

  <bean
      id="lms.rnc.relatorioNaoConformidadeService"
      class="com.mercurio.lms.rnc.report.RelatorioNaoConformidadeService"
  >

    <property name="reportName">
      <value>com/mercurio/lms/rnc/report/emitirRelatoriosRNC.jasper</value>
    </property>
  </bean>

  <!--
     To include additional bean definitions for Spring in the generated
     application context file, add a file to your XDoclet merge directory
     called spring-beans.xml that contains the <bean></bean> markup.
  -->

</beans>
