<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC
    "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans
  default-autowire="byType"
  default-lazy-init="true"
  default-dependency-check="none"
>

  <bean
      id="lms.configuracoes.emitirOcorrenciaAtualizacaoAutomaticaEnderecoPadraoService"
      class="com.mercurio.lms.configuracoes.report.EmitirOcorrenciaAtualizacaoAutomaticaEnderecoPadraoService"
  >

    <property name="reportName">
      <value>com/mercurio/lms/configuracoes/report/emitirOcorrenciaAtualizacaoAutomaticaEnderecoPadrao.jasper</value>
    </property>
  </bean>

  <bean
      id="lms.configuracoes.emitirControleFormulariosService"
      class="com.mercurio.lms.configuracoes.report.EmitirControleFormulariosService"
  >

    <property name="reportName">
      <value>com/mercurio/lms/configuracoes/report/emitirControleFormularios.jasper</value>
    </property>
  </bean>

  <!--
     To include additional bean definitions for Spring in the generated
     application context file, add a file to your XDoclet merge directory
     called spring-beans.xml that contains the <bean></bean> markup.
  -->

</beans>
